    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "devDependencies": {
    "@txo/commitlint": "^1.0.5",
    "@txo/semantic-release": "^1.0.1",
    "@types/eslint": "^8.44.6",
    "@types/jest": "^29.5.7",
    "@types/react": "^18.2.33",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
