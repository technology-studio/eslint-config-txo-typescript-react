{
  "name": "eslint-config-txo-typescript-react",
  "version": "1.2.60",
  "description": "Technology Studio - Eslint typescript react config",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/technology-studio/eslint-config-txo-typescript-react.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://github.com/technology-studio/"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint --ext .jsx,.js,.ts,.tsx .",
    "fixcode": "eslint --ext .jsx,.js,.ts,.tsx . --fix",
    "git-hook": "yarn -s lint",
    "compile": "tsc",
    "prepare": "husky install && yarn compile",
    "sanity": "yarn lint && yarn test && tsc --noEmit && yarn outdated && echo 'success'",
    "semantic-release": "semantic-release",
    "test": "jest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "eslint-config-txo-typescript": "^3.4.0"
